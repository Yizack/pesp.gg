  },
  "bugs": {
    "url": "https://github.com/Yizack/pesp.gg/issues"
  },
  "scripts": {
    "build": "pnpm email:templates && pnpm build:ckeditor && nuxt build",
    "build:ckeditor": "cd assets/ckeditor && pnpm install && pnpm build",
    "dev": "nuxt dev --port=5173",
    "dev:cf": "wrangler dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --coverage",
    "db:migration": "drizzle-kit generate:sqlite",
    "email:templates": "node ./scripts/emailTemplates.js"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.7.4",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/turnstile": "^0.5.0",
    "better-sqlite3": "^8.5.0",
    "chalk": "^5.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "nodemailer": "^6.9.4",
    "nuxt": "^3.6.5",
    "stylelint": "^15.10.2",
    "xml": "^1.0.1"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-vue": "^5.1.0",
    "@glidejs/glide": "^3.6.0",
    "bootstrap": "^5.3.1",
    "chart.js": "^4.3.2",
    "drizzle-kit": "^0.19.12",
