{
  "name": "pesp.gg",
  "version": "2.0.0",
  "description": "Repositorio de la p√°gina web de PESP.GG",
  "author": "Yizack Rangel",
  "license": "MIT",
  "type": "module",
  "homepage": "https://pesp.gg",
  "repository": {
    "type": "git",
    "url": "https://github.com/pespgg/pesp.gg"
  },
  "bugs": {
    "url": "https://github.com/pespgg/pesp.gg/issues"
  },
  "scripts": {
    "build": "npm run email:templates && npm run build:ckeditor && nuxt build",
    "build:ckeditor": "cd assets/ckeditor && pnpm install && npm run build",
    "dev": "nuxt dev --port=5173 --host",
    "dev:cf": "wrangler dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "npm run email:templates && npm run build:ckeditor && nuxt prepare",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --coverage",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.json",
    "email:templates": "tsc ./scripts/emailTemplates.ts --module nodenext && node ./scripts/emailTemplates.js",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --fix --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.css\" --fix --config .config/stylelint.json"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxtjs/sitemap": "^5.2.0",
    "@nuxtjs/turnstile": "^0.8.0",
    "@types/bootstrap": "^5.2.10",
    "@types/glidejs__glide": "^3.6.5",
    "@types/mustache": "^4.2.5",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "eslint": "^9.5.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemailer": "^6.9.14",
    "nuxt": "^3.12.2",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
    "xml": "^1.0.1"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-vue": "^5.1.0",
    "@glidejs/glide": "^3.6.1",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.3",
    "drizzle-kit": "^0.22.7",
    "drizzle-orm": "^0.31.2",
    "mustache": "^4.2.0",
    "nuxt-aos": "^1.2.4",
    "nuxt-icon": "^0.6.10",
    "nuxt-twemoji": "^3.5.2"
  },
  "overrides": {
    "vue": "latest"
  },
  "jest": {
    "coverageProvider": "v8",
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/$1"
    }
  },
  "packageManager": "pnpm@9.4.0"
}
